{
  "name": "dawen/js-logger-bundle",
  "type": "symfony-bundle",
  "description": "A bundle that catches javascript errors and passes them to monolog for Symfony",
  "keywords": ["javascript", "monolog", "logger", "symfony"],
  "homepage": "https://github.com/da-wen/js-logger-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Wendlandt",
      "email": "dan.wendlandt@gmail.com"
    }
  ],
  "require": {
    "php": "~7.1.0",
    "symfony/framework-bundle": "~2.8|~3.0|~4.0",
    "symfony/monolog-bundle": "~2.8",
    "symfony/twig-bundle": "~2.8|~3.0|~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "doctrine/common": "^2.8"
  },
  "autoload": {
    "psr-0": { "Dawen\\Bundle\\JsLoggerBundle": "" }
  },
  "target-dir": "Dawen/Bundle/JsLoggerBundle",
  "minimum-stability": "stable"
}
